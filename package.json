{
    "name": "mbu-online",
    "version": "4.0.3",
    "description": "MBU Registration Website",
    "author": "Domenic Murtari",
    "scripts": {
        "serve": "node dist/out-tsc/server/app.js",
        "build": "npm run tslint && npm run build-ts && npm run client:build",
        "api-test": "NODE_ENV=test && jest -i --config ./tests/jest.config.js",
        "build-ts": "tsc -p \"src/server/tsconfig.json\"",
        "client:build": "webpack --config src/client/webpack.prod.js",
        "client:serve": "webpack-dev-server --config src/client/webpack.dev.js",
        "client:test": "jest --silent --config src/client/jest.config.js",
        "debug": "npm run build && npm run watch-debug",
        "postinstall": "npm run build",
        "serve-debug": "nodemon --inspect dist/out-tsc/server/app.js",
        "start": "npm run serve",
        "test": "NODE_ENV=test && npm run build-ts && npm run unit && npm run api-test",
        "tslint": "tslint -c tslint.json -p \"src/server/tsconfig.json\"",
        "unit": "NODE_ENV=test && jest --config ./src/server/jest.config.js",
        "watch": "npm run build-ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "watch-node": "nodemon dist/out-tsc/server/app.js",
        "watch-test": "NODE_ENV=test && jest -i --watch --config ./tests/jest.config.js",
        "watch-ts": "tsc -w -p \"src/server/tsconfig.json\""
    },
